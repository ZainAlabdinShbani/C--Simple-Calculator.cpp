#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    int x,z,v;
    cin>>z;
    for(int i=0;i<z;i++){
        cin>>x;
        if (x==0)
            cout<<0;
        while(x>0){
            v=x%10;
            x/=10;
            cout<<v<<" ";
        }
        cout<<endl;
    }
    return 0;
}
