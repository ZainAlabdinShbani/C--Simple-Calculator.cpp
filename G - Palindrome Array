#include <iostream>
#include <bits/stdc++.h>
using namespace std;

int main()
{
  int n;
  cin>>n;
  int a[n];
  bool s=true;
  for(int i=0;i<n;i++){
    cin>>a[i];
  }
  for(int i=0,j=n-1;i<n/2;i++,j--){
    if (a[i]==a[j]){
        continue;
  }
    else
        s=false;
        break;
  }
  if(s==true){
    cout<<"YES";
  }
  else
    cout<<"NO";
    return 0;

}
