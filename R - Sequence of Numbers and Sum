#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int x,y,sum;
    for(;;){
        cin>>x>>y;
        if (x<=0 ||y<=0)
            return 0;
        else{
            sum=0;
            int a=min(x,y),b=max(x,y);
            for(a;a<=b;a++){
                cout<<a<<" ";
                sum+=a;
            }
            cout<<"sum ="<<sum<<endl;

        }
    }
    return 0;
}
