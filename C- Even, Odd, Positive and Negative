#include <iostream>

using namespace std;

int main()
{
    signed int n,e=0,o=0,p=0,N=0,x;
    cin>>n;
    for (int i=0;i<n;i++){
        cin>>x;
        if (x%2==0)
            e++;
        else o++;
        if (x>0)
            p++;
        else N++;
    }
    cout<<"Even: "<<e<<"\nOdd: "<<o<<"\nPositive: "<<p<<"\nNegative: "<<N<<endl;
    return 0;
}
