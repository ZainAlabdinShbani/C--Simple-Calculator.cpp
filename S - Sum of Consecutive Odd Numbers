#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int x,y,z,sum;
    cin>>x;
    for(int i=0;i<x;i++){
        cin>>y>>z;
        int a=min(y,z)+1,b=max(y,z);
        sum=0;
        for(a;a<b;a++){
            if (a%2!=0){
                sum+=a;
            }
        }
            cout<<sum<<endl;
        }




    return 0;
}
