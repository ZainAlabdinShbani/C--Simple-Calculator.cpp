#include <iostream>


using namespace std;

int main()
{
  int a,b,c;
  char x,y;
  cin>>a>>x>>b>>y>>c;
  if (x==43){
    if ((a+b)==c)
        cout<<"Yes\n";
    else
        cout<<a+b<<endl;
  }
  else if(x==45){
    if((a-b)==c)
        cout<<"Yes\n";
    else
        cout<<a-b<<endl;
  }
  else if (x==42){
    if((a*b)==c)
        cout<<"Yes\n";
    else
        cout<<a*b<<endl;
  }

    return 0;
}
