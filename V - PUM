#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n,count=1;
    cin>>n;
    for(int i=0;i<n;i++){
        for(int j=0;j<4;j++){
            if(j==3){
                cout<<"PUM\n";
            }
            else
                cout<<count<<" ";
            count++;
        }
    }




    return 0;
}
