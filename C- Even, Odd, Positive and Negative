#include <iostream>

using namespace std;

int main()
{
    long long int  x;
    int e=0,o=0,p=0,N=0,n;
    cin>>n;
    for (int i=0;i<n;i++){
        cin>>x;
        if (x%2==0)
            e++;
        else o++;
        if (x>0 )
            p++;
        else if (x<0)
            N++;
    }
    cout<<"Even: "<<e<<"\nOdd: "<<o<<"\nPositive: "<<p<<"\nNegative: "<<N<<endl;
    return 0;
}
