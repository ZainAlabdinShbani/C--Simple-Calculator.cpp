#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int a,b;
    string s;
    cin>>a>>b;
    cin>>s;
    bool ok;
    for(int i=0;i<s.size();i++){
       if(i==a){
            continue;
            }
       else if(s[i]!='-' && s[a]=='-'){
            ok=true;
       }
       else{
        ok=false;
        break;
       }
    }
    if(ok)
        cout<<"Yes\n";
    else
        cout<<"No\n";
    return 0;
}
